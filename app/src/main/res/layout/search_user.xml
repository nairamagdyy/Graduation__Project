<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"  xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_height="match_parent" >
    <LinearLayout android:id="@+id/search_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff">

                <RelativeLayout android:id="@+id/text_input___filled"
                    android:layout_marginTop="60dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >

                    <View
                        android:id="@+id/rectangle_33"
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_height="56dp"
                        android:layout_marginRight="20dp"
                        android:background="@drawable/rectangle_33_shape" />

                    <ImageButton android:id="@+id/backk"
                        android:src="@drawable/ic_back"
                        android:background="#ffffff"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="6dp"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="15dp" />

                    <RelativeLayout android:id="@+id/close_circle_1"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="290dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="231.32dp">


                        <RelativeLayout android:id="@+id/layer_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >




                        </RelativeLayout>

                    </RelativeLayout>

                    <EditText android:id="@+id/search"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:layout_marginLeft="160dp"
                        android:layout_marginTop="13dp"
                        android:layout_width="wrap_content"
                        android:layout_height="38.5dp"
                        android:hint="Search "
                        />
                    <RelativeLayout android:id="@+id/group_2_ek11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="90dp"
                        android:layout_marginTop="18dp"
                        >

                        <Button
                            android:id="@+id/ellipse_ek22"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/ellipse_ek22_shape" />

                        <View android:id="@+id/rectangle_2_ek4"
                            android:background="@drawable/rectangle_2_ek4_shape"
                            android:layout_marginStart="15dp"
                            android:layout_marginTop="13dp"
                            android:layout_width="2.9dp"
                            android:layout_height="9.11dp"
                            android:layout_marginLeft="0dp" />

                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/navigation_bar_ek2"
                        android:layout_marginTop="800dp"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:background="#ffffff"
                        >

                        <RelativeLayout android:id="@+id/iconly_bold_home_ek2"
                            android:layout_marginStart="40dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="26dp"
                            android:layout_marginTop="15dp">


                            <RelativeLayout android:id="@+id/home_ek10"
                                android:layout_marginTop="2.5dp"
                                android:layout_marginStart="3.13dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3.13dp">

                                <ImageButton android:id="@+id/home1"
                                    android:src="@drawable/home_ek11"
                                    android:contentDescription="home_ek11"
                                    android:layout_width="23.75dp"
                                    android:layout_height="25dp" />

                            </RelativeLayout>

                        </RelativeLayout>




                        <com.blogspot.atifsoftwares.circularimageview.CircularImageView
                            android:id="@+id/profile1"
                            android:layout_alignParentRight="true"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginTop="15dp"
                            android:layout_marginRight="30dp"
                            android:src="@drawable/ic_logout"
                            app:c_border="true"
                            app:c_border_color="@color/colorGray1"
                            app:c_border_width="1dp" />

                    </RelativeLayout>


                </RelativeLayout>

                </RelativeLayout>

    </LinearLayout>
</LinearLayout>