<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent" >
    <LinearLayout android:id="@+id/search_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff">


                <RelativeLayout android:id="@+id/primary___small"
                    android:layout_marginTop="140dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    >

                    <Button android:id="@+id/meals"
                        android:textSize="17sp"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Meals"
                        android:textAllCaps="false"
                        android:layout_marginEnd="194dp"
                        android:layout_marginRight="194dp"
                        android:background="#DCDCDC"/>

                </RelativeLayout>


                <RelativeLayout android:id="@+id/primary___small_ek1"
                    android:layout_marginTop="140dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="194dp"
                    android:layout_alignParentRight="true"
                    >

                    <Button android:id="@+id/users"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:shadowRadius="4"
                        android:shadowColor="#ffffff"
                        android:shadowDx="0"
                        android:shadowDy="4"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Users"
                        android:background="#3CB371"
                        android:textAllCaps="false"
                        />

                </RelativeLayout>

                <TextView android:id="@+id/choirul_syafril"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:textColor="#000000"
                    android:layout_marginLeft="78dp"
                    android:layout_marginTop="226dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="result"
                    />

                <ImageButton android:id="@+id/avatar"
                    android:src="@drawable/avatar"
                    android:background="#ffffff"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="215dp"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="15dp" />


                <RelativeLayout android:id="@+id/text_input___filled"
                    android:layout_marginTop="60dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >


                    <View
                        android:id="@+id/rectangle_33"
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_height="56dp"
                        android:layout_marginRight="20dp"
                        android:background="@drawable/rectangle_33_shape" />

                    <ImageButton android:id="@+id/backk"
                        android:src="@drawable/ic_back"
                        android:background="#ffffff"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="6dp"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="15dp" />

                    <RelativeLayout android:id="@+id/close_circle_1"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="290dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="231.32dp">


                        <RelativeLayout android:id="@+id/layer_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >


                            <RelativeLayout
                                android:id="@+id/close_circle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <ImageButton
                                    android:id="@+id/vector_ek1"
                                    android:layout_width="25.28dp"
                                    android:layout_height="20dp"
                                    android:layout_marginLeft="40dp"
                                    android:layout_marginTop="4dp"
                                    android:src="@drawable/ic_baseline_cancel_24" />

                            </RelativeLayout>

                        </RelativeLayout>

                    </RelativeLayout>

                    <EditText android:id="@+id/search"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:layout_marginLeft="120dp"
                        android:layout_marginTop="13dp"
                        android:layout_width="wrap_content"
                        android:layout_height="38.5dp"
                        android:hint="Search "
                        />
                    <RelativeLayout android:id="@+id/group_2_ek11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="90dp"
                        android:layout_marginTop="18dp"
                        >

                        <Button
                            android:id="@+id/ellipse_ek22"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/ellipse_ek22_shape" />

                        <View android:id="@+id/rectangle_2_ek4"
                            android:background="@drawable/rectangle_2_ek4_shape"
                            android:layout_marginStart="15dp"
                            android:layout_marginTop="13dp"
                            android:layout_width="2.9dp"
                            android:layout_height="9.11dp"
                            android:layout_marginLeft="0dp" />

                    </RelativeLayout>

                    <View android:id="@+id/spacing"
                        android:background="@drawable/spacing_shape"
                        android:layout_marginTop="119dp"
                        android:layout_width="match_parent"
                        android:layout_height="8dp"
                      />

                </RelativeLayout>

                </RelativeLayout>

    </LinearLayout>
</LinearLayout>